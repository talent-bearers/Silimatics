{
  "multipart": [
    {   "apply": { "model": "smedry:rashidPanePost" }},
    {   "when": { "north": true },
      "apply": { "model": "smedry:rashidPaneSide" }
    },
    {   "when": { "east": true },
      "apply": { "model": "smedry:rashidPaneSide", "y": 90 }
    },
    {   "when": { "south": true },
      "apply": { "model": "smedry:rashidPaneSideAlt" }
    },
    {   "when": { "west": true },
      "apply": { "model": "smedry:rashidPaneSideAlt", "y": 90 }
    },
    {   "when": { "north": false },
      "apply": { "model": "smedry:rashidPaneNoside" }
    },
    {   "when": { "east": false },
      "apply": { "model": "smedry:rashidPaneNosideAlt" }
    },
    {   "when": { "south": false },
      "apply": { "model": "smedry:rashidPaneNosideAlt", "y": 90 }
    },
    {   "when": { "west": false },
      "apply": { "model": "smedry:rashidPaneNoside", "y": 270 }
    }
  ]
}
