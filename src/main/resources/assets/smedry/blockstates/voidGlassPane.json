{
  "multipart": [
    {   "apply": { "model": "smedry:basePanePost" }},
    {   "when": { "north": true },
      "apply": { "model": "smedry:basePaneSide" }
    },
    {   "when": { "east": true },
      "apply": { "model": "smedry:basePaneSide", "y": 90 }
    },
    {   "when": { "south": true },
      "apply": { "model": "smedry:basePaneSideAlt" }
    },
    {   "when": { "west": true },
      "apply": { "model": "smedry:basePaneSideAlt", "y": 90 }
    },
    {   "when": { "north": false },
      "apply": { "model": "smedry:basePaneNoside" }
    },
    {   "when": { "east": false },
      "apply": { "model": "smedry:basePaneNosideAlt" }
    },
    {   "when": { "south": false },
      "apply": { "model": "smedry:basePaneNosideAlt", "y": 90 }
    },
    {   "when": { "west": false },
      "apply": { "model": "smedry:basePaneNoside", "y": 270 }
    }
  ]
}
